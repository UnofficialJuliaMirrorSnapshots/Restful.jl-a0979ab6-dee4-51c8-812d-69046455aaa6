<!doctype html>
<html>
<head>
    <title> $title </title>
</head>
<body>
    <ul>
        $$ for i in 1:10 $$
            <li> $i </li>
        $$ end $$
    </ul>
    $$ if true $$
        <div>
            $$ for i in 1:3 $$
                <span> \$(i+1)=$(i+1) </span>
            $$ end $$
        </div>
    $$ else $$
        <div> 错误 </div>
    $$ end $$
</body>
</html>
